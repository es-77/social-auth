{
    "name": "emmanuel-saleem/social-auth",
    "description": "Laravel social authentication package with Google, Microsoft and more OAuth providers using Socialite",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "EmmanuelSaleem\\Auth\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Emmanuel Saleem",
            "email": "emmanuelsaleem098765@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "laravel/socialite": "^5.0",
        "socialiteproviders/microsoft": "^4.0"
    },
    "require-dev": {
        "laravel/passport": "^11.0|^12.0"
    },
    "suggest": {
        "laravel/sanctum": "Required for Sanctum-based API authentication (default)",
        "laravel/passport": "Required for Passport-based OAuth2 authentication"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EmmanuelSaleem\\SocialAuth\\SocialAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}