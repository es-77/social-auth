{
    "name": "emmanuel-saleem/social-auth",
    "description": "Laravel social authentication package with Google, Microsoft and more OAuth providers using Socialite",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "EmmanuelSaleem\\Auth\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Your Name",
            "email": "your@email.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "laravel/socialite": "^5.0",
        "socialiteproviders/microsoft": "^4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EmmanuelSaleem\\SocialAuth\\SocialAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}